{
    "version": 2,
    "builds": [
        {
            "src": "index.html",
            "use": "@vercel/static"
        },
        {
            "src": "src/main.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/favicon.ico",
            "dest": "/favicon.ico"
        },
        {
            "src": "/src/(.*)",
            "dest": "/src/$1"
        },
        {
            "src": "/node_modules/(.*)",
            "dest": "/node_modules/$1"
        }
    ]
}
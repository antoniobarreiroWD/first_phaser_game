{
    "version": 2,
    "builds": [
        {
            "src": "index.html",
            "use": "@vercel/static"
        },
        {
            "src": "src/main.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/favicon.ico",
            "dest": "/favicon.ico"
        },
        {
            "src": "/main.js",
            "dest": "src/main.js"
        }
    ]
}